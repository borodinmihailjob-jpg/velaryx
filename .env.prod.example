POSTGRES_DB=astrobot
POSTGRES_USER=astrobot
POSTGRES_PASSWORD=change_me
DATABASE_URL=postgresql+psycopg2://astrobot:change_me@postgres:5432/astrobot
REDIS_URL=redis://redis:6379/0

API_HOST=0.0.0.0
API_PORT=8000

BOT_TOKEN=replace_me
BOT_USERNAME=replace_me_bot
MINI_APP_NAME=app
MINI_APP_PUBLIC_BASE_URL=https://your-host.tailnet.ts.net/
INTERNAL_API_KEY=replace_internal_key
# Bot -> API internal sync (used to save Telegram language_code on /start)
INTERNAL_API_BASE_URL=http://api:8000

REQUIRE_TELEGRAM_INIT_DATA=true
ALLOW_INSECURE_DEV_AUTH=false
TELEGRAM_INIT_DATA_MAX_AGE_SECONDS=900
TELEGRAM_BOT_API_TIMEOUT_SECONDS=15

CORS_ORIGINS_RAW=https://your-host.tailnet.ts.net

LOCAL_ONLY_MODE=false
ASTROLOGY_PROVIDER=swisseph
TAROT_PROVIDER=local

ENABLE_RESPONSE_LOCALIZATION=true
TRANSLATE_VIA_GOOGLE_FREE=false
TRANSLATION_TIMEOUT_SECONDS=8

LLM_PROVIDER=openrouter
OLLAMA_MODEL=qwen2.5:7b
# If Ollama runs in compose profile: http://ollama:11434
# If Ollama runs on host machine:   http://host.docker.internal:11434
OLLAMA_BASE_URL=http://host.docker.internal:11434
OLLAMA_TIMEOUT_SECONDS=60

VITE_API_BASE_URL=/api
VITE_ALLOW_DEV_AUTH=false
VITE_TAROT_LOADING_GIF=/tarot-loader.gif
VITE_BOT_USERNAME=replace_me_bot
VITE_APP_NAME=app
OPENROUTER_API_KEY=sk-or-v1-replace_me
STARS_PRICE_NATAL_PREMIUM=49
STARS_PRICE_TAROT_PREMIUM=29
STARS_PRICE_NUMEROLOGY_PREMIUM=29
# Free model for non-premium features (tarot/stories/basic natal/basic numerology)
OPENROUTER_FREE_MODEL=meta-llama/llama-3.2-3b-instruct,mistralai/mistral-nemo,google/gemma-3n-e4b-it,qwen/qwen-2.5-7b-instruct
# Premium features continue to use OPENROUTER_MODEL from backend defaults unless overridden.
